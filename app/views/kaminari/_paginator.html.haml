= paginator.render do
  %nav
    %ul.pagination.justify-content-center
      = first_page_tag
      = prev_page_tag
      - each_page do |page|
        - if page.display_tag?
          = page_tag page
        - elsif !page.was_truncated?
          = gap_tag
      - unless current_page.out_of_range?
        = next_page_tag
        = last_page_tag
